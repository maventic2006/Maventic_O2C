<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:f="sap.ui.layout.form"
    xmlns:l="sap.ui.layout"
    xmlns:core="sap.ui.core">

    
    <Dialog title="Bill To Address">
        <Table items="{mPageData>/billToAddressList}"
            itemPress="onCustomerAddressSelectionPress" width="600px">
            <columns>
                <Column>
                    <Text text="Code" />
                </Column>
                <Column>
                    <Text text="Address" />
                </Column>
                <Column>
                    <Text text="GSTIN" />
                </Column>
            </columns>
            <ColumnListItem type="Active">
                <cells>
                    <Text class="sapUiTinyMarginBegin" text="{mPageData>cust_address_id}" />
                </cells>
                <cells>
                    <Text class="sapUiTinyMarginBegin" text="{mPageData>cust_address}" />
                </cells>
                <cells>
                    <Text class="sapUiTinyMarginBegin" text="{mPageData>cust_add_gstin}" />
                </cells>
            </ColumnListItem>
        </Table>

        <buttons>
            <Button text="Cancel" press="onPopupDialogCloseBtnPress" />
        </buttons>
    </Dialog>


</core:FragmentDefinition>